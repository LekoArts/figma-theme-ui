{
  "name": "figma-theme-ui",
  "version": "2.0.0",
  "description": "Turn your Theme UI config into styles",
  "author": "LekoArts <lekoarts@gmail.com>",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:ci": "jest --ci --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint --ignore-path .gitignore . --ext .ts,.js",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@figma/plugin-typings": "^1.78.1",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.18.1",
    "@types/theme-ui": "^0.6.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cssnano": "^5.1.15",
    "csstype": "^3.1.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.2.1",
    "figma-plugin-ds-svelte": "^1.4.3",
    "jest": "^29.6.4",
    "parse-color": "^1.0.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "rollup": "^3.29.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "superstruct": "1.0.3",
    "svelte": "^3.59.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  }
}
