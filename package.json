{
  "name": "figma-theme-ui",
  "version": "2.0.0",
  "description": "Turn your Theme UI config into styles",
  "author": "LekoArts <lekoarts@gmail.com>",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:ci": "jest --ci --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint --ignore-path .gitignore . --ext .ts,.js",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@figma/plugin-typings": "^1.57.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^3.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.4",
    "@types/theme-ui": "^0.6.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cssnano": "^5.1.14",
    "csstype": "^3.1.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "figma-plugin-ds-svelte": "^1.4.3",
    "jest": "^27.5.1",
    "parse-color": "^1.0.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "rollup": "^2.79.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "superstruct": "1.0.3",
    "svelte": "^3.55.0",
    "ts-jest": "^27.1.5",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
